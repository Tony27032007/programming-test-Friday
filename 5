n=input("name:")
b=[]
x=0
y=0
for i in range (0,3): 
      l=input("score:")
      b.append(l)
for i in range (0,3):
      y=float(b[i])
      x=x+y
x=x/3
if x>=90:
      print("A")
elif x<90 and x>=80:
      print("B")
elif x<80 and x>=70:
      print("C")
elif x<70 and x>=60:
      print("D")
elif x<60:
      print("F")